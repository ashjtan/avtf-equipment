<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="350.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="chappelle.five.view.AdminWelcomeScreenController">
   <children>
      <Label layoutX="209.0" layoutY="47.0" text="Welcome, Admin">
         <font>
            <Font name="Helvetica Neue Thin" size="25.0" />
         </font>
      </Label>
      <Button fx:id="btnEquipmentList" layoutX="83.0" layoutY="175.0" mnemonicParsing="false" onAction="#equipmentList" prefHeight="62.0" prefWidth="176.0" text="View Equipment Info" textOverrun="CLIP">
         <font>
            <Font name="Helvetica Neue Thin" size="16.0" />
         </font>
      </Button>
      <Button fx:id="btnStudentInfo" layoutX="344.0" layoutY="175.0" mnemonicParsing="false" onAction="#studentInfo" prefHeight="62.0" prefWidth="176.0" text="View Student Info" textOverrun="CLIP">
         <font>
            <Font name="Helvetica Neue Thin" size="16.0" />
         </font>
      </Button>
      <Button fx:id="btnLogOut" layoutX="267.0" layoutY="286.0" mnemonicParsing="false" onAction="#handleLogout" text="Log Out">
         <font>
            <Font name="Helvetica Neue Thin" size="13.0" />
         </font>
      </Button>
      <Label layoutX="210.0" layoutY="119.0" text="What would you like to do?">
         <font>
            <Font name="Helvetica Neue Thin" size="16.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
